{
  "expo": {
    "name": "Unibility",
    "slug": "Unibility",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ]
  },
  "name": "Unibility",
  "ios" : {
    "infoPlist": {
      "UIBackgroundModes": [
        "location"
      ],
      "NSLocationAlwaysUsageDescription": "This app uses location for map capabilities and to add reports at current location"
    }
  },
  "android": {
    "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION", "FOREGROUND_SERVICE"]
  }
}
